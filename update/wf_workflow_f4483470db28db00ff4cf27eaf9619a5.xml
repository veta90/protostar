        <value>   answer = ifScript();&#13;
&#13;
   function ifScript() {&#13;
      if (current.odometer&gt;=4500 &amp;&amp; !current.first_maintenance_notification_sent) {&#13;
		 vehicle.first_maintenance_notification_sent=true;&#13;
         return 'yes';&#13;
