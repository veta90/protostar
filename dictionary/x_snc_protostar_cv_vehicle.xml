<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Vehicle" max_length="40" name="x_snc_protostar_cv_vehicle" type="collection">
        <element label="API Access Token" mandatory="true" max_length="200" name="api_access_token" type="string"/>
        <element label="API Car ID" mandatory="true" max_length="40" name="api_car_id" type="string"/>
        <element choice="3" default="Not qualified" label="First Maintenance Status" max_length="1000" name="first_maintenance_status" type="choice">
            <choice>
                <element label="Not qualified" sequence="1" value="Not qualified"/>
                <element label="Notification sent" sequence="2" value="Notification sent"/>
                <element label="Booking done" sequence="3" value="Booking done"/>
                <element label="Maintenance Performed" sequence="4" value="Maintenance Performed"/>
            </choice>
        </element>
        <element attributes="ignore_filter_on_new=true" default="javascript:global.getNextObjNumberPadded();" label="Number" max_length="40" name="number" type="string"/>
        <element label="Odometer" max_length="20" name="odometer" read_only="true" type="decimal"/>
        <element choice="3" default="Not qualified" display="true" label="Second Maintenance Status" max_length="1000" name="second_maintenance_status" type="choice">
            <choice>
                <element label="Not qualified" sequence="1" value="Not qualified"/>
                <element label="Notification sent" sequence="2" value="Notification sent"/>
                <element label="Booking done" sequence="3" value="Booking done"/>
                <element label="Maintenance Performed" sequence="4" value="Maintenance Performed"/>
            </choice>
        </element>
        <element label="User" mandatory="true" max_length="32" name="user" reference="sys_user" type="reference"/>
        <index name="index">
            <element name="user"/>
        </index>
    </element>
</database>
